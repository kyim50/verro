{
  "expo": {
    "name": "Erato",
    "slug": "erato",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kyim50.verro",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "verrocio.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true
            },
            "api.verrocio.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.kyim50.erato",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share artwork."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": false
        }
      ]
    ],
    "scheme": "erato",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c28d6897-a427-4bbb-8f35-74abae16eef0"
      },
      "EXPO_PUBLIC_API_URL": "https://api.verrocio.com/api",
      "EXPO_PUBLIC_SOCKET_URL": "https://api.verrocio.com",
      "EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY": "pk_live_51SiVLYRxHT8ws7mzT4SDJq9luDms1qe8RC2mNpQtYfTVv5gzrGTOGGFsGuiLMlJmJSYthp73Ar7lrnOUMVKLCgCf00gp5sEqpa"
    },
    "owner": "kyim50"
  }
}
